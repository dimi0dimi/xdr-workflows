                        "unique_name": "definition_activity_02SX8U8OZS2LY2hgbXjnIPOaCHjnZrspIXE",
                        "name": "Execute Python Script",
                        "title": "Get Updated Chat History",
                        "type": "python3.script",
                        "base_type": "activity",
                        "properties": {
                          "action_timeout": 180,
                          "continue_on_failure": false,
                          "display_name": "Get Updated Chat History",
                          "script": "import sys, json\n\ntool_message = json.loads(sys.argv[1])\ntool_name = str(sys.argv[2])\ntool_result = json.loads(sys.argv[3])\nchat_history = json.loads (sys.argv[4])\ntool_output = [i.get(\"value\") for i in tool_result if i.get(\"name\") == \"Output\"][0]\n\nupdated_chat_history = chat_history.copy()\nupdated_chat_history[\"messages\"].append(tool_message)\n\ntool_calls = tool_message.get(\"tool_calls\") or []\nupdated_chat_history[\"messages\"].extend([{\"role\": \"tool\", \"tool_call_id\": tc[\"id\"], \"content\": str(tool_output)} for tc in tool_calls])\n\noutput = json.dumps(updated_chat_history)",
                          "script_arguments": [
                            "$activity.definition_activity_02SX8UDB2R3AR0ItoIF2x6HTUztyE2gz6ad.output.jsonpath_queries.message$",
